{
  "name": "my-blog",
  "type": "module",
  "version": "1.1.0",
  "scripts": {
    "dev": "TINA_SKIP_SCHEMA_VALIDATION=true tinacms dev -c \"astro dev\"",
    "start": "TINA_SKIP_SCHEMA_VALIDATION=true tinacms dev -c \"astro dev\"",
    "build": "TINA_SKIP_SCHEMA_VALIDATION=true tinacms build && astro build && find ./dist -name *.map -delete",
    "build:delete": "TINA_SKIP_SCHEMA_VALIDATION=true tinacms build && astro build && find ./dist -name *.map -delete",
    "build:astro-only": "astro build && find ./dist -name *.map -delete",
    "preview": "astro preview",
    "astro": "astro",
    "e2e:all": "playwright test",
    "e2e:chromium": "playwright test --project=chromium",
    "prepare": "is-ci || husky install",
    "format": "prettier --write --plugin=prettier-plugin-astro \"**/*.{css,ts,tsx,astro}\"",
    "lint:js": "eslint \"./src/**/*.{js,jsx,ts,tsx,astro}\" --fix --ignore-path .gitignore",
    "lint:style": "stylelint \"./src/**/*.{css,scss,astro}\" --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@astrojs/markdown-remark": "^6.3.1",
    "@astrojs/node": "^9.2.1",
    "@astrojs/react": "^4.2.7",
    "@astrojs/rss": "^4.0.0",
    "@astrojs/sitemap": "^3.3.1",
    "@astrojs/tailwind": "^6.0.2",
    "@sentry/browser": "^7.51.2",
    "@sentry/node": "^7.51.2",
    "@tailwindcss/typography": "^0.5.9",
    "@tinacms/cli": "^1.9.5",
    "astro": "^5.7.10",
    "astro-icon": "^0.8.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "hastscript": "^9.0.1",
    "plaiceholder": "^2.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-parse": "^9.0.1",
    "rehype-stringify": "^10.0.1",
    "sharp": "^0.32.1",
    "tailwindcss": "^3.3.2",
    "tinacms": "^2.7.5",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "zenn-content-css": "^0.1.159",
    "zenn-markdown-html": "^0.1.159"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@playwright/test": "^1.33.0",
    "@sentry/vite-plugin": "^0.7.2",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^15.5.1",
    "postcss-html": "^1.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "scss": "^0.2.4",
    "stylelint": "^16.19.1",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-no-unsupported-browser-features": "^8.0.4",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.11.1",
    "typescript": "^5.0.0",
    "vite": "^6.3.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write --plugin=prettier-plugin-astro",
      "eslint --cache --fix"
    ],
    "*.astro": [
      "prettier --write --plugin=prettier-plugin-astro",
      "stylelint --fix",
      "eslint --cache --fix"
    ],
    "*.{css,scss}": [
      "prettier --write --plugin=prettier-plugin-astro",
      "stylelint --fix"
    ],
    "*.json": [
      "prettier --write --plugin=prettier-plugin-astro"
    ]
  }
}
