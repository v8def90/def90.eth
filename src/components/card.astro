---
import Custompicture from '@components/custompicture.astro';

const { url, src, alt = '', title } = Astro.props;
---

<article class="card">
  <a class="text-inherit no-underline hover:underline" rel="prefetch" href={url}>
    <Custompicture src={src} alt={alt} />
    <h2 class="text-base font-normal text-left">{title}</h2>
  </a>
</article>
